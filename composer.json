{
    "name": "bluebaytravel/dosh",
    "description": "Simple currency formatter for PHP.",
    "license": "MIT",
    "authors": [{
        "name": "James Brooks",
        "email": "james@bluebaytravel.co.uk"
    }],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "BlueBayTravel\\Dosh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlueBayTravel\\Tests\\Dosh\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
